<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Roster.
    -->
    <changeSet id="20191116011832-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="category_id"
                                 baseTableName="roster"
                                 constraintName="fk_roster_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="category"/>

        <addForeignKeyConstraint baseColumnNames="roster_id"
                                 baseTableName="roster_player"
                                 constraintName="fk_roster_player_roster_id"
                                 referencedColumnNames="id"
                                 referencedTableName="roster"/>

        <addForeignKeyConstraint baseColumnNames="player_id"
                                 baseTableName="roster_player"
                                 constraintName="fk_roster_player_player_id"
                                 referencedColumnNames="id"
                                 referencedTableName="player"/>
        
    </changeSet>
</databaseChangeLog>
